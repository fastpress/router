{
    "name": "fastpress/router",
    "description": "A simple yet powerful routing component for the Fastpress framework, enabling efficient HTTP routing in PHP applications.",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "scripts": {
        "test": "phpunit",
        "fix-style": "phpcbf --standard=PSR12 src/",
        "lint": "phpcs --standard=PSR12 src/",
        "analyze": "phpstan analyse src/"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Fastpress\\Routing\\": "src/Routing/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Samson Daniel",
            "email": "samayo@pm.me"
        }
    ],
    "keywords": ["http", "router", "php", "fastpress", "routing", "framework"],
    "homepage": "https://github.com/fastpress/router",
    "support": {
        "issues": "https://github.com/fastpress/router/issues",
        "source": "https://github.com/fastpress/router"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
